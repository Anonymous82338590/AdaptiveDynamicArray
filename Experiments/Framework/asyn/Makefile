all: asyn2 asyn3 asyn4 asyn5 asyn6 asynNoNewConn asyn updateInThread syn


asyn2: asyn2.cpp ../../../HeaderFiles/DynamicArray.cpp ../../../HeaderFiles/DynamicArray.h
#	g++ -std=c++11 ../../../HeaderFiles/StandardArray.cpp ../../../HeaderFiles/DynamicArray.cpp \
	../../../HeaderFiles/LinkedList.cpp ../../../HeaderFiles/CountedBtree.cpp insert.cpp -o insert \
	-L/usr/include/mysql -lmysqlclient -I/usr/include/mysql
	g++ -std=c++11 -Wall -g ../../../HeaderFiles/DynamicArray.cpp asyn2.cpp \
    -I/usr/include/cppconn -o asyn2 -pthread -L/usr/lib -lmysqlcppconn

